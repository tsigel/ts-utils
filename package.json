{
  "name": "ts-utils",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsigel/ts-utils.git"
  },
  "scripts": {
    "build": "sh scripts/build",
    "compile": "sh scripts/clean && sh scripts/compile",
    "init": "sh scripts/postinstall",
    "pretest": "npm run compile",
    "prepublush": "npm run test && npm run build",
    "postversion": "git push origin HEAD && git push --tags && npm publish",
    "test": "node_modules/karma/bin/karma start && ./node_modules/.bin/istanbul cover _mocha",
    "test-dev": "npm run pretest && ./node_modules/.bin/istanbul cover _mocha",
    "test-debug": "node_modules/karma/bin/karma start --singleRun false --autoWatch true",
    "doc": "node_modules/.bin/typedoc --out ./doc --mode file ./src"
  },
  "description": "Some utils for array irerate",
  "main": "dist/utils.js",
  "types": "index.d.ts",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "browserify": "^14.4.0",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.5",
    "karma": "^1.7.0",
    "karma-commonjs": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "mocha": "^3.4.2",
    "phantomjs": "^2.1.7",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.14",
    "tslint": "^5.5.0",
    "typedoc": "^0.7.1",
    "typescript": "^2.4.1",
    "uglify-js": "^3.0.25"
  },
  "author": "Daniil Tsigelnitskiy",
  "license": "ISC",
  "dependencies": {}
}
